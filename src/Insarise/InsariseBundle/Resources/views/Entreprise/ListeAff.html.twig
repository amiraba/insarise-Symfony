<script type="text/javascript">
function getEtape(id)
{
  $.ajax({
       url : "{{ path('entreprise_cntrEtape')}}",
       type : "POST",
       data : "idp=" + id ,
       success : function(code_html){ 
         
         $('#data').html(code_html);
                 }
    });
}
function methode(id,ind)
{
  $.ajax({
       url : "{{ path('entreprise_validate')}}",
       type : "POST",
       data : "id=" + id +"&ind=" + ind,
       success : function(code_html){ 
         var id = "#";
         var a = id + 1;
         var h = id.concat(a);
         var res = id.concat(id);
         $(h).hide();
        $(res).html('validated');
                 }
    });
}
</script>

